extends layout

block content
  h1 Tweets from @testingtwitbot
  p Welcome
  each val in results
    ul.tweetList
      li(id=val.id)=val.tweets
      form(method=GET, action=`/tweets/${val.id}`)
        input#tweetId(type='hidden', value=val.tweets name='status')
        input(type='submit' value='retweet')
